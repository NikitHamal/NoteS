<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:background="#F8F8F8">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="56dp"
		android:padding="8dp"
		android:gravity="center_vertical"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/back_button"
			android:layout_width="40dp"
			android:layout_height="40dp"
			android:padding="8dp"
			android:src="@drawable/icon_arrow_back_round"
			android:scaleType="fitXY" />
		<TextView
			android:id="@+id/textview1"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal|center_vertical"
			android:text="Settings"
			android:textSize="20sp"
			android:textColor="#000000"
			android:layout_weight="1" />
		<View
			android:id="@+id/view2"
			android:layout_width="40dp"
			android:layout_height="40dp" />
	</LinearLayout>
	<ScrollView
		android:id="@+id/vscroll1"
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="16dp"
			android:orientation="vertical">
			<TextView
				android:id="@+id/textview2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="8dp"
				android:text="Account"
				android:textSize="18sp"
				android:textColor="#7C4DFF" />
			<LinearLayout
				android:id="@+id/profile_setting"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="8dp"
				android:padding="16dp"
				android:background="@drawable/card_background"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview1"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_person_round"
					android:scaleType="fitXY"
					android:tint="#333333" />
				<TextView
					android:id="@+id/textview3"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:text="Profile"
					android:textSize="16sp"
					android:textColor="#333333"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/imageview2"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_chevron_right_round"
					android:scaleType="fitXY"
					android:tint="#757575" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/sync_setting"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="16dp"
				android:padding="16dp"
				android:background="@drawable/card_background"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview3"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_sync_round"
					android:scaleType="fitXY"
					android:tint="#333333" />
				<TextView
					android:id="@+id/textview4"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:text="Sync"
					android:textSize="16sp"
					android:textColor="#333333"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/imageview4"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_chevron_right_round"
					android:scaleType="fitXY"
					android:tint="#757575" />
			</LinearLayout>
			<TextView
				android:id="@+id/textview5"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="8dp"
				android:text="Appearance"
				android:textSize="18sp"
				android:textColor="#7C4DFF" />
			<LinearLayout
				android:id="@+id/theme_setting"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="8dp"
				android:padding="16dp"
				android:background="@drawable/card_background"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview5"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_palette_round"
					android:scaleType="fitXY"
					android:tint="#333333" />
				<TextView
					android:id="@+id/textview6"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:text="Theme"
					android:textSize="16sp"
					android:textColor="#333333"
					android:layout_weight="1" />
				<TextView
					android:id="@+id/textview7"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Light"
					android:textSize="14sp"
					android:textColor="#757575" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/font_setting"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="16dp"
				android:padding="16dp"
				android:background="@drawable/card_background"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview6"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_text_format_round"
					android:scaleType="fitXY"
					android:tint="#333333" />
				<TextView
					android:id="@+id/textview8"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:text="Font"
					android:textSize="16sp"
					android:textColor="#333333"
					android:layout_weight="1" />
				<TextView
					android:id="@+id/textview9"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Default"
					android:textSize="14sp"
					android:textColor="#757575" />
			</LinearLayout>
			<TextView
				android:id="@+id/textview10"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="8dp"
				android:text="Other"
				android:textSize="18sp"
				android:textColor="#7C4DFF" />
			<LinearLayout
				android:id="@+id/about_setting"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="8dp"
				android:padding="16dp"
				android:background="@drawable/card_background"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview7"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_info_round"
					android:scaleType="fitXY"
					android:tint="#333333" />
				<TextView
					android:id="@+id/textview11"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:text="About"
					android:textSize="16sp"
					android:textColor="#333333"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/imageview8"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_chevron_right_round"
					android:scaleType="fitXY"
					android:tint="#757575" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/help_setting"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="8dp"
				android:padding="16dp"
				android:background="@drawable/card_background"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview9"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_help_round"
					android:scaleType="fitXY"
					android:tint="#333333" />
				<TextView
					android:id="@+id/textview12"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:text="Help"
					android:textSize="16sp"
					android:textColor="#333333"
					android:layout_weight="1" />
				<ImageView
					android:id="@+id/imageview10"
					android:layout_width="24dp"
					android:layout_height="24dp"
					android:src="@drawable/icon_chevron_right_round"
					android:scaleType="fitXY"
					android:tint="#757575" />
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
